(set-info :original "mytest2.bc")
(set-info :authors "SeaHorn v.0.1.0-rc3")
(set-logic HORN)
(declare-fun main@entry () Bool)
(declare-fun main@entry.split () Bool)
(declare-fun verifier.error (Bool Bool Bool) Bool)
(assert (verifier.error false false false))
(assert (verifier.error false true true))
(assert (verifier.error true false true))
(assert (verifier.error true true true))
(assert main@entry)
(assert (forall ((@llvm.used_0 (_ BitVec 32))
         (main@%_0_0 (_ BitVec 32))
         (main@%_1_0 Bool)
         (main@%_2_0 Bool))
  (=> (and main@entry
           true
           (bvugt @llvm.used_0 #x00000000)
           (= main@%_1_0 (bvsgt main@%_0_0 #x00000002))
           (not main@%_1_0)
           (= main@%_2_0 (xor main@%_1_0 true)))
      main@entry.split)))
(assert (not (exists ((@llvm.used_0 (_ BitVec 32))
              (main@%_0_0 (_ BitVec 32))
              (main@%_1_0 Bool)
              (main@%_2_0 Bool))
       (and main@entry.split))))
(check-sat)

